{
    "name": "dear-imgui",
    "version": "1.91.8",
    "description": "Bloat-free Graphical User interface for C++ with minimal dependencies",
    "homepage": "https://github.com/ocornut/imgui",
    "license": "MIT",
    "dependencies":
    [
        {
            "name": "vcpkg-cmake",
            "host": true
        },
        {
            "name": "vcpkg-cmake-config",
            "host": true
        },
        {
            "name": "myvr-vcpkg-cmake",
            "host": true
        }
    ],
    "default-features":
    [
        "with-internal",
        "with-stb-textedit",
        "backend-opengl"
    ],
    "features":
    {
        "with-internal":
        {
            "description": "Using internal header(s)"
        },
        "with-stb-textedit":
        {
            "description": "Using STB textedit"
        },
        "backend-opengl":
        {
            "description": "Using OpenGL"
        },
        "backend-vulkan":
        {
            "description": "Using Vulkan"
        },
        "backend-glfw":
        {
            "description": "Using GLFW",
            "dependencies":
            [
                "glfw"
            ]
        },
        "platform-win32":
        {
            "description": "Support for win32"
        }
    }
}
